Table configuraciones{

id int [pk, increment]
nombre varchar
descripcion varchar
direccion varchar
telefono varchar
divisa varchar
correo_electronico varchar
web varchar
logo varchar
created_at timestamp
updated_at timestamp
}

Table gestiones{
    id int [pk, increment]
    nombre varchar
    created_at timestamp
    updated_at timestamp
}

Table niveles{
    id int [pk, increment]
    nombre varchar
    created_at timestamp
    updated_at timestamp
}

Table turnos{
    id int [pk, increment]
    nombre varchar
    created_at timestamp
    updated_at timestamp
}
